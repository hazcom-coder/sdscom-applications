@page "{id:long?}"
@model SDSCom.Pages.Administrator.ApplicationSettingDetailModel
@{
    ViewData["Title"] = "ApplicationSettingDetail";
}

<h2>Application Setting Detail</h2>

<form method="post">

    <input id="txtID" asp-for="@Model.ApplicationSetting.Id" class="hidden" />

    <div class="form-group">
        <label>Area:</label>
        <input id="txtAreA" asp-for="@Model.ApplicationSetting.Area" class="form-control" />
    </div>

    <div class="form-group">
        <label>Setting:</label>
        <input id="txtSetting" asp-for="@Model.ApplicationSetting.Setting" class="form-control" />
    </div>

    <div class="form-group">
        <label>Value:</label>
        <input id="txtDV" asp-for="@Model.ApplicationSetting.DataValue" class="form-control" />
    </div>
    <hr />
    <div id="divSaveDelete" class="form-group row">
        <div class="col-md-4 left">
            <input type="submit" class="btn-warning" id="deleteUser" value="Delete" asp-page-handler="Delete" />
        </div>
        <div class="col right">
            <input type="submit" class="btn-primary" id="saveUser" value="Save" asp-page-handler="Save" />
        </div>
    </div>

</form>

